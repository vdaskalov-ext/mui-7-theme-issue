    },
    "@emotion/styled": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "private": false,
  "module": "./esm/index.js",
  "exports": {
    "./package.json": "./package.json",
    ".": {
      "require": {
        "types": "./index.d.ts",
        "default": "./index.js"
      },
      "import": {
        "types": "./esm/index.d.ts",
        "default": "./esm/index.js"
      }
    },
    "./*": {
      "require": {
        "types": "./*/index.d.ts",
        "default": "./*/index.js"
      },
      "import": {
        "types": "./esm/*/index.d.ts",
        "default": "./esm/*/index.js"
      }
    },
    "./esm": null,
    "./modern": null
  },
  "types": "./index.d.ts"
